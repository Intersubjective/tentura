### GQL signIn
POST http://localhost:2080/api/v2/graphql
Content-Type: application/json
X-Request-Type: GraphQL

mutation SignIn($authRequestToken: String!) {
  signIn(
    authRequestToken: $authRequestToken
  ) {
    subject
    expires_in
    access_token
  }
}

