#
### GQL signIn
#
POST {{base_url}}/{{gqlV2_path}}
Content-Type: application/json
X-Request-Type: GraphQL

mutation SignIn($authRequestToken: String!) {
  signIn(
    authRequestToken: $authRequestToken
  ) {
    subject
    expires_in
    access_token
  }
}

