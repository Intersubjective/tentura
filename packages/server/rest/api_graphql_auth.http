### GQL signUp
POST http://localhost:2080/api/v2/graphql
# POST https://dev.tentura.io/api/v1/graphql
Content-Type: application/json
X-Request-Type: GraphQL

mutation SignUp(
  $authRequestToken: String!,
  $title: String!,
  $description: String
) {
  signUp(
    authRequestToken: $authRequestToken,
    title: $title,
    description: $description
  ) {
    subject
    expires_in
    access_token
  }
}

