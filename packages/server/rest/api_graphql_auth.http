#
### GQL signUp
#
POST {{base_url}}/{{gqlV2_path}}
Content-Type: application/json
X-Request-Type: GraphQL

mutation SignUp(
  $authRequestToken: String!,
  $title: String!,
  $description: String
) {
  signUp(
    authRequestToken: $authRequestToken,
    title: $title,
    description: $description
  ) {
    subject
    expires_in
    access_token
  }
}

